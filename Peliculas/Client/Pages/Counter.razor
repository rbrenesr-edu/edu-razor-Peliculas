@page "/counter"

<PageTitle>Counter</PageTitle>

<h1>Counter</h1>
<p role="status">Current count: @currentCount</p>
<p role="status">Singleton count: @servicioSingleton.Valor</p>
<p role="status">Transient count: @servicioTransient.Valor</p>
<button class="btn btn-primary" @onclick="IncrementCount">Click me</button>
<button class="btn btn-primary" @onclick="IncrementCountJavaScript">Click me JS</button>

